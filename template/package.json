{
    "name": "rdc-regular-suite",
    "description": "UI components based on RegularJS",
    "version": "0.6.2",
    "main": "./dist/{{{suiteName}}}.js",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "keywords": [
        "regular",
        "component",
        "ui"
    ],
    "scripts": {
        "prerelease": "npm login",
        "release": "npm run build && npm publish",
        "build": "webpack --config build/webpack.prod.conf.js",
        "postbuild": "cp -R ./dist ../app/dist",
        "serve": "webpack-dev-server --config build/webpack.dev.conf.js",
        "lint": "eslint --fix -c ./.eslintrc.js --ext .js",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "dependencies": {
        "bowser": "^1.5.0",
        "clipboard": "^1.6.0",
        "dom-align": "^1.5.2",
        "js-calendar": "^1.2.3",
        "moment": "^2.22.2",
        "nek-ui": "^0.6.1",
        "popper.js": "^1.14.3",
        "postcss-salad": "^2.0.1",
        "qs": "^6.7.0",
        "regular-state": "^0.6.0-beta.8",
        "regularjs": "0.4.3",
        "reqwest": "^2.0.5",
        "resize-observer-polyfill": "^1.5.0",
        "scroll-into-view": "^1.9.1",
        "validator": "^6.1.0"
    },
    "devDependencies": {
        "babel-core": "^6.22.1",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^7.1.5",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.3.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.22.0",
        "babel-register": "^6.22.0",
        "browser-sync": "^2.18.2",
        "chalk": "^2.4.2",
        "clean-webpack-plugin": "^2.0.2",
        "codemirror-highlight": "^1.1.1",
        "css-loader": "^0.26.1",
        "eslint": "^5.5.0",
        "eslint-config-kaola": "^0.1.3",
        "eslint-friendly-formatter": "^4.0.1",
        "eslint-loader": "^2.1.0",
        "eslint-plugin-html": "^4.0.5",
        "extract-text-webpack-plugin": "^2.1.2",
        "fs-extra": "^2.1.2",
        "glob": "^7.1.1",
        "happypack": "^5.0.1",
        "hexo": "3.3.7",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^0.14.2",
        "less": "^3.8.1",
        "less-loader": "^4.1.0",
        "lint-staged": "^4.0.0",
        "mkdirp": "^0.5.1",
        "np": "^2.16.0",
        "npm-run-all": "^3.1.1",
        "peta": "^1.0.2",
        "postcss-import": "^11.1.0",
        "postcss-loader": "^2.1.3",
        "progress-bar-webpack-plugin": "^1.12.1",
        "rimraf": "^2.5.4",
        "run-sequence": "^1.2.1",
        "style-loader": "^0.13.1",
        "text-loader": "0.0.1",
        "url-loader": "^1.0.1",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.2",
        "webpack-dev-middleware": "^3.3.0",
        "webpack-dev-server": "^3.4.1",
        "webpack-merge": "^4.1.0",
        "yargs": "^6.6.0"
    }
